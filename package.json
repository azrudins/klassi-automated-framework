      "**/page-objects/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "homepage": "https://github.com/larryg01/webdriverio-cucumber-js#readme",
  "dependencies": {
    "chai": "^4.1.2",
    "chalk": "^2.3.0",

    "commander": "^2.11.0",
    "cucumber": "^4.0.0",
    "cucumber-html-reporter": "^3.0.4",
    "fs-extra-promise": "^1.0.1",
    "fs-plus": "^3.0.1",
    "geckodriver": "^1.10.0",
    "merge": "^1.2.0",
    "moment": "^2.20.1",
    "node-dir": "^0.1.17",
    "nodemailer": "^4.3.1",
    "phantomjs-prebuilt": "^2.1.16",
    "random-words": "0.0.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "require-dir": "^0.3.2",
    "targz": "^1.0.1",
